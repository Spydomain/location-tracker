# Runtime environment for location tracker
PORT=5000

# If you have a hosted deployment, set this to skip ngrok (must be HTTPS)
BASE_URL=

# ngrok auth token (used by CLI and pyngrok fallback)
NGROK_AUTHTOKEN= 

# Auto-open the Latest dashboard in a browser (true/false)
AUTO_OPEN_LATEST=false

# Twilio (optional)
TWILIO_SID=
TWILIO_TOKEN=
TWILIO_FROM=

# Optional defaults
TARGET_NUMBER=
MESSAGE_PREFIX="Hi! Please tap this secure link to share your location: "
IP_GEOLOOKUP=false

TWILIO_SID=
TWILIO_TOKEN=
TWILIO_FROM=
